		$("#searchBtn").click(function() {
  			var keyword = $("#inputKeyword").val();
			var searchOption = $("#searchOption").val();
			if(keyword.trim().length>0) {
				//if(searchOption=='article') {
					/*window.location.href = '*///etw.nextdigital.com.hk//*search/article/?keyword=' + encodeURIComponent(keyword);*/
