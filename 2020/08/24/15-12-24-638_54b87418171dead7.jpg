<script>
$(function(){
	$('.ImageLazyLoad').each(function(){
		if ($(this).attr('actual') != ''){
			$(this).attr('src', $(this).attr('actual'));
		}
	});

	$('.ImageLazyLoadBackground').each(function(){
		if ($(this).attr('actual') != ''){
            $('.ImageLazyLoadBackground').css('display', 'block');
            $('.ImageLazyLoadBackground.no-big-image').css('display', 'none');
            var cssAttribute = "url(" + $(this).attr('actual') + ")";
            $(this).css('background-image' , cssAttribute);
		} else{
            $('.ImageLazyLoadBackground').css('display', 'none');
            $('.ImageLazyLoadBackground.no-big-image').css('display', 'block');
        }
	});

})	
</script>

