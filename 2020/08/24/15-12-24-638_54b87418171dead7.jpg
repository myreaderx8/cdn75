 function String2Hex(tmp) {
     var str = '';
     for(var i = 0; i < tmp.length; i++) {
         str += tmp[i].charCodeAt(0).toString(16);
     }
     return str;
 }

 function setCookie(cname, cvalue, exdays) {
     var d = new Date();
     d.setTime(d.getTime() + (exdays*24*60*60*1000));
     //d.setTime(d.getTime() + (60 * 1000)); //Test with 1 min expire
     var expires = "expires="+d.toUTCString();
     document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/;domain="+"etw.nextdigital.com.hk";
 }

 // handle subscription (HETW-502)
 function setUreadCookie(cname, cvalue) {
    var timestamp = new Date();
    timestamp.setHours(23);
    timestamp.setMinutes(59);
    timestamp.setSeconds(59);
    var expires = "expires="+timestamp.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/;domain="+"etw.nextdigital.com.hk";
}

 function getCookie(cname) {
     var name = cname + "=";
     var ca = document.cookie.split(';');
     for(var i = 0; i < ca.length; i++) {
         var c = ca[i];
         while (c.charAt(0) == ' ') {
             c = c.substring(1);
         }
         if (c.indexOf(name) == 0) {
                       return c.substring(name.length, c.length);
         }
     }
     return "";
 }

