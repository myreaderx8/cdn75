	<script>
	// handle ads according to subscription (HETW-502)
	var pendingAdsElmIds = [];
	function displayAdsAfterCheckBlocking(elmId) {
		pendingAdsElmIds.push(elmId);
	}
	$(function(){
		function preloadMobileAds(blocked){
			if (blocked) {
				return;
			}
			googletag.cmd.push(function() {
						googletag.defineSlot('/7350/ETW_mobilesite/index/FixedBanner1', [[320, 50], [300, 250]], 'div-FixedBanner1').addService(googletag.pubads());
        	        	        				        	        		googletag.defineSlot('/7350/ETW_mobilesite/index/inRead', [1, 1], 'div-inRead').addService(googletag.pubads());
        											googletag.pubads().enableSingleRequest();
       	 		googletag.pubads().collapseEmptyDivs();
         		googletag.enableServices();
				//googletag.pubads().refresh();
														googletag.display('div-FixedBanner1');
																			googletag.display('div-inRead');
												 });
		}
		function preloadDesktopAds(blocked){
			googletag.cmd.push(function() {
							      googletag.defineSlot('/7350/ETW_web/index/HeadBanner', [[970, 90], [970, 250], [970, 160], [728, 90]], 'div-HeadBanner').addService(googletag.pubads());
        						        		  googletag.defineSlot('/7350/ETW_web/index/LREC1', [[300, 250], [300, 430], [300, 600]], 'div-LREC1').addService(googletag.pubads());
        						         			googletag.defineSlot('/7350/ETW_web/index/Skyscraper1', [[120, 600], [160, 600]], 'div-Skyscraper1').addService(googletag.pubads());
        						        			googletag.defineSlot('/7350/ETW_web/index/Skyscraper2', [[120, 600], [160, 600]], 'div-Skyscraper2').addService(googletag.pubads());
       							googletag.pubads().enableSingleRequest();
       	 		googletag.pubads().collapseEmptyDivs();
         		googletag.enableServices();
									googletag.display('div-HeadBanner');
									googletag.display('div-LREC1');
									googletag.display('div-Skyscraper1');
									googletag.display('div-Skyscraper2');
									googletag.display('div-inRead');
							 });
			//googletag.pubads().refresh();
		}
		var isMobileLoaded = false;
		var isDesktopLoaded = false;
		var isBlocked = false;
		var reloadWidth = 970;

		function initAds() {
			var currentWidth = $( window ).width();
			if (currentWidth < reloadWidth ){
				preloadMobileAds(isBlocked);
				isMobileLoaded = true;
			}else{
				preloadDesktopAds(isBlocked);
				isDesktopLoaded = true;
			}

			$( window ).resize(function() {
				if( $( window ).width() < reloadWidth){
					if(!isMobileLoaded){
						preloadMobileAds(isBlocked);
						isMobileLoaded = true;
					}
				}else{
					if(!isDesktopLoaded){
						preloadDesktopAds(isBlocked);
						isDesktopLoaded = true;
					}
				}
			});

			if (!isBlocked) {
				pendingAdsElmIds.forEach(function(elmId) {
					googletag.cmd.push(function() { googletag.display(elmId); });
				});
			}
		}

		if (typeof isBlockedContentPromise === "function") {
			isBlockedContentPromise()
				.then(function(blocked) {
					isBlocked = blocked;
					initAds();
				})
				.catch(function(err) {
					initAds();
				});
		} else {
			initAds();
		}
	});
	</script>
