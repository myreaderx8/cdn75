<!-- Skyscraper -->
<script type="text/javascript">
	var $window = $(window);
   	var doorLeft = $('#div-Skyscraper1');
    var doorRight = $('#div-Skyscraper2');
    var documentHeight = $(document).height();
    var topDefaultMargin = 138;
    $(window).scroll(function() {
    	var scrollTop = $window.scrollTop();
    	//var leftTopPos = doorLeft.offset().top;
    	//var rightTopPos = doorRight.offset().top;

		if(scrollTop > topDefaultMargin) {
    		var topValue = scrollTop - topDefaultMargin + 106;
    		if(typeof doorLeft !== "undefined") doorLeft.css('top', topValue+'px');
    		if(typeof doorRight !== "undefined") doorRight.css('top', topValue+'px');
    	} else{
    		if(typeof doorLeft !== "undefined") doorLeft.css('top', '0');
    		if(typeof doorRight !== "undefined") doorRight.css('top', '0');
    	}
   	}).trigger('scroll');
</script>
