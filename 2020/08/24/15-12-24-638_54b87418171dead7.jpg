        // display choose item
        $('.search-box-lv3 input').on('click', function () {
            $(this).parent().addClass('selected');
            var classes = $(this).parents('.search-box-lv2').attr('class').replace('search-box-lv2', '').trim();
            var keyword = $(this).parent().text().trim();
            if($(this).prop('checked') === true){
                var htmlContent = '<span class="choose-item '+ classes +'">'+ keyword +'<i class="icon-del">x</i></span>';
                $('.choose-group').append(htmlContent);
            } else{
                $('.choose-group>.choose-item').each(function () {
                    var itemTxt = $(this).text().trim();
                    if(itemTxt.indexOf(keyword) !== (-1)){
                        $(this).remove();
                    }
                });
            }
        });
        // remove display item
        $('.choose-group').on('click', '.choose-item', function () {
            var thisSpan = $(this).text().replace('x', '').trim();
            var classes = $(this).attr('class').replace('choose-item', '').trim();
            if(classes === 'price'){
                $('#slider-range').data("ionRangeSlider").reset();
                $('#amount').val('任何');
                $('#amount').data('value','');
            } else {
                var labelArr= $('.search-box-lv2.'+classes).find('.selected');
                $(labelArr).each(function () {
                    if($(this).text().trim() === thisSpan){
                        $(this).children().prop('checked', false);
                    }
                });
                $('.search-box-lv2.'+classes).find('input').each(function () {
                    $(this).attr("disabled", false);
                    $(".shareerror").hide();
