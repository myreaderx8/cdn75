				//}
                setHistoryItems(keyword);
                var link = '//etw.nextdigital.com.hk/search/article/?keyword=' + encodeURIComponent(keyword);
				window.location.href = appendFromSource(link);
			}
		});

		$("#advanSearchBtn").click(function(e) {
			e.preventDefault();
			var area = [], district = [], cuisinetype = [], cuisine = [], foodtype = [], foodgroup=[],spending = [];
            $.each($("input[name='selected_district']:checked"), function() {
                if($(this).hasClass('areaOption')) {
					area.push($(this).val());
				}
				else {
					district.push($(this).val());
				}
            });
			$.each($("input[name='selected_foodtype']:checked"), function() {
                if($(this).hasClass('allOption')){
                    foodgroup.push($(this).val());
                }
                else {
                    foodtype.push($(this).val());
                }
			});
			$.each($("input[name='selected_cuisine']:checked"), function() {
                if($(this).hasClass('allOption')) {
					cuisinetype.push($(this).val());
				}
				else {
					cuisine.push($(this).val());
				}
            });
			/*$.each($("input[name='selected_spending']:checked"), function() {
                spending.push($(this).val());
            });*/
            $.each($('#amount'), function () {
                spending.push($(this).data('value'));
            });
			
			/* Fixed don't choose any item not allowed to search */
			if((area.length===0) && (district.length===0) && (cuisinetype.length===0) && (cuisine.length===0) && (foodtype.length===0) && (foodgroup.length===0)){
				$(".shareerror").text('請輸入分類搜尋資料').show();
				return false;
			}
			
			var queryParam = {
				'area':area.join(","),
				'district':district.join(","),
				'cuisinetype':cuisinetype.join(","),
				'cuisine':cuisine.join(","),
				'foodtype':foodtype.join(","),
                'foodgroup':foodgroup.join(","),
				'spending':spending.join(",")
			};

			var queryStr = decodeURIComponent(jQuery.param(queryParam));
			if(queryStr.trim().length>0) {
                var link = '//etw.nextdigital.com.hk/search/restaurant/?' + queryStr;
				window.location.href = appendFromSource(link);
			}
		});

		$("input[name=selected_district]").change(function() {
			if($("input[name=selected_district]:checked").length>=3) {
				$("input[name=selected_district]").each(function() {
					if($(this).prop('checked')==false) {
						$(this).attr("disabled", true);
                        $(".shareerror").text('最多可以選擇3項').show();
                    }
				});
			}
			else {
				$("input[name=selected_district]").each(function() {
					$(this).attr("disabled", false);
                    $(".shareerror").hide();

                });
			}
		});

		$("input[name=selected_foodtype]").change(function() {
            //-- HKMAGST-3973 block both cuisine and foodtype search--
