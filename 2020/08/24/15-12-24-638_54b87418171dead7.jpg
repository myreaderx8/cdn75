// handle utm parameter (HETW-508/509)
function ureadPrepareParams(){
	var resu = {
		utm_source: 'etw_web', 
		utm_medium: 'internal', 
		utm_campaign: 'etw_profile', 
		utm_term: '', 
		utm_content: ''
	};

	var urlparams = window.location.search.substring(1);
	urlparams = urlparams.split('&');
	if(urlparams.length==1){
		return resu;
	}
	for (var i = 0; i < urlparams.length; i++)
	{
		var sParameterName = urlparams[i].split('=');
		switch(sParameterName[0]){
			case 'utm_source':
				resu['utm_source'] = sParameterName[1];
			break;
			case 'itm_source':
				resu['utm_source'] = sParameterName[1];
			break;

			case 'utm_medium':
				resu['utm_medium'] = sParameterName[1];
			break;
			case 'itm_medium':
				resu['utm_medium'] = sParameterName[1];
			break;

			case 'utm_campaign':
				resu['utm_campaign'] = sParameterName[1];
			break;
			case 'itm_campaign':
				resu['utm_campaign'] = sParameterName[1];
			break;

			case 'utm_content':
				resu['utm_content'] = sParameterName[1];
			break;
			case 'itm_content':
				resu['utm_content'] = sParameterName[1];
			break;

			case 'utm_term':
				resu['utm_term'] = sParameterName[1];
			break;
			case 'itm_term':
				resu['utm_term'] = sParameterName[1];
			break;
		};
	}
	if(resu['utm_source'] == ''){
		return resu;
	}
	return resu;
}
