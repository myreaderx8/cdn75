                if($("input[name=selected_cuisine]:checked").length>0) {
                    $('ul.search-box-lv1 li.foodtype').addClass("disabled");
                }else{
                    $('ul.search-box-lv1 li.foodtype').removeClass("disabled");
                }
            }
            $(this).remove();
        });

        // /* slider range */
        var priceStr = $('.price-val').text().trim();
        var priceArr = priceStr.split(',');
        let priceString = priceArr.map(function(price){ if(price == "0"){return "$"+0;} return "$"+String(parseInt(price)-1)});
        priceString.push("$"+priceArr[priceArr.length-1]+'以上');  // 添加一个代表无限的数值
        var $range = $("#slider-range");

        $range.ionRangeSlider({
            type: "double",
            grid: true,
            from: priceString[0],
            to: priceString[priceString.length-1],
            values: priceString,
            hide_from_to: false,
            hide_min_max: true,
            min_interval: 1,
            onStart: function (data) {
                if((data.from === 0) && (data.to === priceString.length-1)) {
                    $('#amount').val('任何');
                } else {
                    $('#amount').val('$' + data.from+ ' - $' + data);
                }
            },
            onFinish: function (data) {
                var priceStr = $('.price-val').text().trim();
                var priceArr = priceStr.split(',');
                priceArr.push('');  // 添加一个代表无限的数值
                var left = data.from;
                var right = data.to;
                if(left === right) {  // 阻止两个handle重合
                    return false;
                } else {
                    if(left === 0 && right === priceArr.length-1) {
                        $('#amount').val('任何');
                    } else if(left === 0){
                        $('#amount').val('$' + (parseInt(priceArr[right])-1) + '以下');
                    } else if(right === priceArr.length-1){
                        $('#amount').val('$' + parseInt(priceArr[left]-1) + '以上');
                    } else{
                        $('#amount').val('$' + parseInt(priceArr[left]-1) + ' - $' + parseInt(priceArr[right]-1));
                    }
                    
                    if(priceArr[right].length==0&&priceArr[left]!='0'){
                        $('#amount').data('value', parseInt(priceArr[left]-1) + ',' + priceArr[right]);
                    }
                    if(priceArr[right].length!=0&&priceArr[left]=='0'){
                        $('#amount').data('value', priceArr[left] + ',' + parseInt(priceArr[right]-1));
                    }
                    if(priceArr[right].length!=0&&priceArr[left]!='0'){
                        $('#amount').data('value', parseInt(priceArr[left]-1) + ',' + parseInt(priceArr[right]-1));
                    }
                    
                }
                // update choose group
                if($('.choose-group>.choose-item').length > 0 && $('.choose-item.price').length > 0){
                    $('.choose-group>.choose-item').each(function () {
                        if($(this).hasClass('price')){
                            $('.choose-item.price').text($('#amount').val());
                            $('.choose-item.price').append('<i class="icon-del">x</i>');
                        }
                    });
                } else if($('.choose-item.price').length === 0) {
                    var htmlContent = '<span class="choose-item price">'+ $('#amount').val() +'<i class="icon-del">x</i></span>';
                    $('.choose-group').append(htmlContent);
                }

            }
        });


        function getCookie(cname) {
         var name = cname + "=";
         var ca = document.cookie.split(';');
         for(var i = 0; i < ca.length; i++) {
             var c = ca[i];
             while (c.charAt(0) == ' ') {
                 c = c.substring(1);
             }
             if (c.indexOf(name) == 0) {
                           return c.substring(name.length, c.length);
             }
         }
         return "";
        }

        function setCookie(cname, cvalue, exdays) {
             var d = new Date();
             d.setTime(d.getTime() + (exdays*24*60*60*1000));
             //d.setTime(d.getTime() + (60 * 1000)); //Test with 1 min expire
             var expires = "expires="+d.toUTCString();
             document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/;domain="+"etw.nextdigital.com.hk";
        }

        function delCookie(cname) {
             var exp = new Date();
             exp.setTime(exp.getTime() - 1);
             var expires = "expires="+exp.toUTCString();
             var cval=getCookie(cname);
             if(cval!=null)
             document.cookie = cname + "=" + cval + ";" + expires + ";path=/;domain="+"etw.nextdigital.com.hk";
        }

